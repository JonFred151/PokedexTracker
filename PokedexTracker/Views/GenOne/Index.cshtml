@model IEnumerable<GenOne>

<h2>Pokedex Track Gen 1</h2>
<table class="table">
    <tr>
        <th>Caught?</th>
        <th>Dex Number</th>
        <th>Pokemon</th>
        <th>Type</th>
        <th>Obtainable</th>
        <th>Method</th>
    </tr>

    @foreach (var GenOne in @Model)
    {
        <tr>
            <td><input name="IsCaught" type="checkbox" /></td>
            <td>@GenOne.Dexid</td>
            <td>@GenOne.Pokemon</td>
            <td>@GenOne.Type</td>
            <td>@GenOne.Obtainable</td>
            <td>@GenOne.Method</td>
        </tr>
    }
</table>

