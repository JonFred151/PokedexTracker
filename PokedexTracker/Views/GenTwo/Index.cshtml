@model IEnumerable<GenTwo>

<h2>Pokedex Track Gen 2</h2>
<table class="table">
    <tr>
        <th>Caught?</th>
        <th>Dex Number</th>
        <th>Pokemon</th>
        <th>Type</th>
        <th>Obtainable</th>
        <th>Method</th>
    </tr>

    @foreach (var GenTwo in @Model)
    {
        <tr>
            <td><input name="IsCaught" type="checkbox" /></td>
            <td>@GenTwo.Dexid</td>
            <td>@GenTwo.Pokemon</td>
            <td>@GenTwo.Type</td>
            <td>@GenTwo.Obtainable</td>
            <td>@GenTwo.Method</td>
        </tr>
    }
</table>


